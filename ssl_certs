# Nginx config with SSL
server {
    listen 443 ssl;
    server_name thienln.vn;

    ssl_certificate /opt/ssl-selfsigned-certs/mycert.crt;
    ssl_certificate_key /opt/ssl-selfsigned-certs/mykey.key;

    ssl_protocols TLSv1.2 TLSv1.3;
    ssl_ciphers 'TLS_AES_128_GCM_SHA256:TLS_AES_256_GCM_SHA384:TLS_CHACHA20_POLY1305_SHA256:ECDHE-RSA-AES128-GCM-SHA256:ECDHE-RSA-AES256-GCM-SHA384';
    ssl_prefer_server_ciphers on;
    ssl_session_cache shared:SSL:10m;
    ssl_session_timeout 10m;

    location / {
        root /var/www/html;
        index index.html;
    }
}


# Nginx config with SSL and redirect http to https
server {
    listen 80;
    server_name thienln.vn;
    return 301 https://$host$request_uri;
}


# Check nginx config
nginx -t

# Reload nginx
nginx -s reload

# Check nginx status
systemctl status nginx

# Check nginx log

tail -f /var/log/nginx/access.log

tail -f /var/log/nginx/error.log

# Check nginx log with specific domain
cat /var/log/nginx/access.log | grep thienln.vn

cat /var/log/nginx/error.log | grep thienln.vn



