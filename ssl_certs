# Generate CA certificate
openssl req -x509 -new -nodes -key myCA.key -sha256 -days 365 -out myCA.pem -subj "/C=VN/ST=HoChiMinhCity/L=District7/O=ThienlnCorporation/OU=Cloud Services Department/CN=myCA"


# Check CA certificate file
openssl x509 -in myCA.pem -noout -text

