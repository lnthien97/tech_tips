# sign cert
openssl x509 -req -in mycsr.csr -CA CA/myCA.pem -CAkey CA/myCA.key -CAcreateserial -out mycert.crt -days 365 -sha256

# check cert file
openssl x509 -in mycert.crt -noout -text

