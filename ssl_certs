# Generate private key
openssl genrsa -out mykey.key 2048

# Check private key file
openssl rsa -in mykey.key -noout -text

# Generate CSR
openssl req -new -key mykey.key -out mycsr.csr -subj "/C=VN/ST=HoChiMinhCity/L=District7/O=ThienlnCorporation/OU=Cloud Services Department/CN=thienln.vn" -addext "subjectAltName = DNS:thienln01.vn, DNS:thienln02.vn"

# check request cert .csr file 
openssl req -in mycsr.csr -noout -text

